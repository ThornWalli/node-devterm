{
  "name": "node-devterm",
  "version": "0.0.0",
  "description": "Useful package when using a DevTerm.",
  "type": "module",
  "main": "./build/esm/index.mjs",
  "exports": {
    ".": {
      "require": "./build/cjs/index.cjs",
      "default": "./build/esm/index.js",
      "import": "./build/esm/index.js"
    },
    "./config": {
      "require": "./build/cjs/config.cjs",
      "default": "./build/esm/config.mjs",
      "import": "./build/esm/config.js"
    }
  },
  "module": "./build/esm/index.mjs",
  "directories": {
    "build": "build"
  },
  "scripts": {
    "postinstall": "npm run build",
    "build": "node ./build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Thorn-Welf Walli <lammpee@gmail.com> (https://lammpee.de)",
  "license": "ISC",
  "dependencies": {
    "canvas": "^2.8.0",
    "consola": "^2.15.3",
    "floyd-steinberg": "^1.0.6",
    "jsbarcode": "^3.11.5",
    "qrcode": "^1.5.0",
    "serialport": "^9.2.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "esbuild": "^0.14.10",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3"
  }
}